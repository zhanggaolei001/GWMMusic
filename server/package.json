{
  "name": "gwm-music-server",
  "version": "0.1.0",
  "private": true,
  "description": "GWM music API gateway with NetEase Cloud Music integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:e2e": "jest --testPathPattern=e2e\\.dev\\.test\\.ts"
  },
  "dependencies": {
    "dotenv": "16.4.7",
    "axios": "^1.6.7",
    "cors": "2.8.5",
    "express": "4.18.2",
    "ffmpeg-static": "5.2.0",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "11.1.1",
    "http-errors": "2.0.0",
    "mime-types": "2.1.35",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "music-metadata": "^7.14.0",
    "netease-cloud-music-api-alger": "file:../vendor",
    "node-id3": "0.2.6"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.12",
    "@types/mime-types": "2.1.4",
    "@types/morgan": "1.9.7",
    "@types/node": "20.11.21",
    "@types/supertest": "2.0.16",
    "jest": "29.7.0",
    "supertest": "6.3.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3"
  }
}