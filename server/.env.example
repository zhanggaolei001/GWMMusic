# GWM NetEase music server environment configuration
# Copy this file to .env and fill in the secrets before running the server.

PORT=4000
HOST=0.0.0.0
PUBLIC_BASE_URL=

# Cache tuning
CACHE_DIR=../cache
CACHE_MAX_SIZE_MB=2048
CACHE_TTL_HOURS=24
CACHE_MIN_SIZE_MB=4
CACHE_MIN_BITRATE_KBPS=192

# NetEase API credentials & networking
NETEASE_COOKIE=MUSIC_U=<YOUR_COOKIE>
NETEASE_REAL_IP=101.42.0.1
NETEASE_PROXY=
NETEASE_TIMEOUT_MS=15000
